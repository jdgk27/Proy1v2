<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.grupo7.proy1.edd.proy1v2.Main">

    <Button
        android:id="@+id/btnrentar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btndev"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="63dp"
        android:onClick="onClickRentar"
        android:text="Rentar"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="73dp" />

    <Button
        android:id="@+id/btndev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="82dp"
        android:onClick="onClickDev"
        android:text="Registrar devolucion"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="184dp"
        android:layout_above="@+id/btnlogout"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/btnlogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="130dp"
        android:onClick="onClickLogout"
        android:text="Cerrar Sesion"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="293dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
